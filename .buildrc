{
  "enableTerminal": true,
  "allowedTargets": [
    "win",
    "linux"
  ],
  "packagePrefix": "@confuscript/",
  "commands": [
    {
      "id": "grammar",
      "type": "custom",
      "package": "lang",
      "command": "grammar",
      "runInEnvironments": []
    },
    {
      "id": "project-schema",
      "type": "custom",
      "package": "core",
      "command": "schema",
      "runInEnvironments": [
        "prod"
      ]
    },
    {
      "id": "tree-schema",
      "type": "custom",
      "package": "core",
      "command": "treeschema",
      "runInEnvironments": [
        "prod"
      ]
    },
    {
      "id": "core",
      "type": "typescript",
      "useAlternateConfig": false,
      "runInEnvironments": [
        "dev",
        "prod"
      ],
      "package": "core"
    },
    {
      "id": "trees",
      "type": "typescript",
      "useAlternateConfig": false,
      "runInEnvironments": [
        "dev",
        "prod"
      ],
      "package": "trees"
    },
    {
      "id": "cli",
      "type": "typescript",
      "useAlternateConfig": false,
      "runInEnvironments": [
        "dev",
        "prod"
      ],
      "package": "cli"
    }
  ]
}
